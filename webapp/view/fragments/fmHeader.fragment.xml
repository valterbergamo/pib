<core:FragmentDefinition
	xmlns:m="sap.m"
    xmlns="sap.m" 
	xmlns:core="sap.ui.core">
		
        <m:Button
            id="sideNavigationTogglem:Button"
            icon="sap-icon://menu2"
            type="Transparent"
            press="onCollapseExpandPress"
            tooltip="Expandir/Retrair">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow"/>
            </layoutData>
        </m:Button>
        <ToolbarSpacer/>
        <Title
            text="GestÃ£o de testes v1.9"
            level="H2"/>
        <ToolbarSpacer/>
        
        <m:Button
            id="fullScresn:Button"
            icon="sap-icon://full-screen"
            type="Transparent"
            press="onFullScreen"
            visible="false"
            tooltip="{i18n>notificationm:ButtonTooltip}">
            <layoutData>
                <OverflowToolbarLayoutData closeOverflowOnInteraction="false"/>
            </layoutData>
        </m:Button>

        <m:Button
            id="exitfullScresn:Button"
            icon="sap-icon://exit-full-screen"
            type="Transparent"
            press="onFullScreen"
            visible="false"
            tooltip="{i18n>notificationm:ButtonTooltip}">
            <layoutData>
                <OverflowToolbarLayoutData closeOverflowOnInteraction="false"/>
            </layoutData>
        </m:Button>

        <m:Button
            id="notificationm:Button"
            icon="sap-icon://ui-notifications"
            visible="false"
            type="Transparent"
            press="onTeste"
            tooltip="{i18n>notificationm:ButtonTooltip}">
            <layoutData>
                <OverflowToolbarLayoutData closeOverflowOnInteraction="false"/>
            </layoutData>
        </m:Button>
        
        <m:Button id="btLogout"     
                text="{token>/Name1}" 
                visible="{token>/Login}"    
                icon="sap-icon://disconnected"  
                press="_onLogout" 
                type="Transparent">
            <layoutData>
                <OverflowToolbarLayoutData closeOverflowOnInteraction="false"/>
            </layoutData>
        </m:Button>
		
        <m:Button 
            id="btLogin"      
            text="Login"                  
            visible="{= ${token>/Login} === false }"     
            icon="sap-icon://visits"               
            press="_onPopupLogin"  
            type="Transparent">
            <layoutData>
                <OverflowToolbarLayoutData closeOverflowOnInteraction="false"/>
            </layoutData>
        </m:Button>

	

</core:FragmentDefinition>